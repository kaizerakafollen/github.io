body
  width 100%
  background-color #000
  font-size 14px
  $FO = 'Fjalla One', sans-serif;
  $if = 'Indie Flower', cursive
  padding 0
  margin 0
.header
  width 80%
  margin 0 auto
  background-color #000
  .logo-main
    float left
    h1
      padding-left 30px
      text-transform uppercase
      font-family 'Fjalla One', sans-serif
      color #fff
    h5
      padding-left 30px
      text-transform capitalize
      font-family 'Indie Flower', cursive
      color #fff
  .nav
    float left
    li
      float right 
      display inline-block
      margin-right 10px
      a
        text-transform uppercase
        
        color #fff
        padding 40px 15px 10px 15px
        transition all 1s
      a:hover
        background #fff
        color #000
        cursor: pointer
.welcome-block
  .error-code
    padding 40px
    font-size 75px
    font-family 'Monoton', cursive
    text-align center
    text-transform uppercase
    text-shadow 0 0 80px red,0 0 30px FireBrick,0 0 6px DarkRed
    color red
    p
        margin 0
.error
    color #fff
    text-shadow 0 0 80px #ffffff,0 0 30px #008000,0 0 6px #0000ff
    &:hover
        text-shadow 0 0 200px #ffffff,0 0 80px #008000,0 0 6px #0000ff
    span
        animation upper 11s linear infinite

.code
    &:hover
        text-shadow 0 0 100px red,0 0 40px FireBrick,0 0 8px DarkRed
    span
        &:nth-of-type(2)
            animation lower 10s linear infinite
        &:nth-of-type(1)
            text-shadow none
            opacity .4
.main-text
  // padding-top 100px
  padding-bottom 100px
  .container
    .text
      text-align center
      color #fff
      background #000
      padding-bottom 30px
      box-shadow 0 38px 44px -41px #fff
      span
        font-size 150px
        margin-bottom 30px
      .sub-text
        font-family 'Indie Flower', cursive
        font-size 30px
        text-align center
      .pencil
        position absolute
        z-index -1
        right 50%
        // top 285px
      .linear
        position absolute
        z-index -1
        // top 310px
        right: 50%;
        margin-right: -35px;
.main-content
  padding-bottom 100px
  .container
    .row
      .portfolio-caption
        font-family 'Indie Flower', cursive
        font-size 30px
        color #fff
        margin-bottom 30px
      .section-foto 
        text-align center
        .foto
          margin-right 25px
          margin-bottom 25px
.footer
  background #000
  .container
    .row
      .about-author
        position relative
        .author 
          // position: absolute; */
          /* bottom: 181px; */
          padding: 10px;
          background: #fff;
          color: #fff;
        .author-name
          color: #fff;
          margin-left: 10px;
          font-family: 'Fjalla One', sans-serif;
          font-style: italic;
          font-size: 24px;
          /* margin-top: -8px; */
          margin-bottom: 19px;
          display: inline-block;
          letter-spacing: 2px;
        .place
          margin-left: -140px;
          margin-top: 49px;
        .name-place 
          margin-left: 12px;
          /* display: inline-block; */
          color: #fff;
          font-size: 14px;
          margin-top: -57px;
          margin-left: 180px;
        .twitter-link
          margin-left: 70px;
          .twitter
            margin-left 100px
        .dribbble-link
          .dribbble
            margin-left 30px
        .about-author-text
          margin-top 20px
          // width 60%
          color #fff
          font-size 14px
          word-spacing 2px
      .panel-exp
        .my-skill
          margin-top 20px
          color #fff
          font-size 30px
        .skill-panel
          position relative
          display block
          margin-bottom 15px
          width 100%
          background #000
          height 35px
          border-radius 3px
          transition 0.4s linear
          transition-property width, background-color
          .skill-panel-title
            position absolute
            top 0
            left 0
            width 90px
            color #fff
            background #000
            // border-top-left-radius:3px;
            // border-bottom-left-radius:3px;
            span
              display block
              background:rgba(0, 0, 0, 0.1);
              padding:0 20px;
              height:35px;
              line-height:35px;
              border-top-left-radius:3px;
              border-bottom-left-radius:3px;
          .skill-panel-panel
            height 35px
            width 0px
            background #fff
            border-radius 3px
          .skill-panel-percent
            position absolute
            right 10px
            top 0px
            font-size 11px
            height 35px
            line-height 35px
            color #fff
// Animation EROR -404
@keyframes upper
  0%,19.999%,22%,62.999%,64%, 64.999%,70%,100% 
    opacity .99
    text-shadow 0 0 80px #ffffff,0 0 30px #008000,0 0 6px #0000ff
  20%,21.999%,63%,63.999%,65%,69.999%
    opacity 0.4
    text-shadow none
@keyframes lower
    0%,12%,18.999%,23%,31.999%,37%,44.999%,46%,49.999%,51%,58.999%,61%,68.999%,71%,85.999%,96%,100% 
      opacity .99
      text-shadow 0 0 80px red,0 0 30px FireBrick,0 0 6px DarkRed
    19%,22.99%,32%,36.999%,45%,45.999%,50%,50.99%,59%,60.999%,69%,70.999%,86%,95.999%
      opacity 0.4
      text-shadow none
.col-xs-12 {
    width: 100%;
}
            
          
        
        
          
      
  
  
        
      
        

        
        